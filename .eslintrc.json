{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint"],
  "extends": ["next/core-web-vitals",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "import/no-unresolved": 1,
    "no-console": [ 1 ],
    "no-debugger": [ 1 ],
    "indent": [ 1, 2, { "SwitchCase": 1 } ],
    "quotes": [ 1, "double" ],
    "semi": [ 1, "always" ],
    "semi-spacing": [ 1 ],
    "one-var": [ 1, { "initialized": "never" } ],
    "no-multi-spaces": [ 1 ],
    "no-empty": [ 1 ],
    "space-infix-ops": [ 1 ],
    "comma-spacing": [ 1 ],
    "spaced-comment": [ 1, "always", { "exceptions": [ "*" ] } ],
    "no-unneeded-ternary": [ 1 ],
    "no-useless-call": [ 1 ],
    "block-scoped-var": [ 1 ],
    "comma-dangle": [ 1, "never" ],
    "comma-style": [ 1, "last" ],
    "space-in-parens": [ 1 ],
    "keyword-spacing": [ 1 ],
    "arrow-spacing": [ 1 ],
    "space-unary-ops": [ 1 ],
    "new-parens": [ 1 ],
    "key-spacing": [ 1 ],
    "eqeqeq": [ 1 ],
    "eol-last": [ 1 ],
    "dot-location": [ 1, "property" ],
    "func-style": [ 1, "expression" ],
    "no-lone-blocks": [ 1 ],
    "no-native-reassign": [ 1 ],
    "no-nested-ternary": [ 1 ],
    "no-fallthrough": [ 1 ],
    "no-undef": [ 1 ],
    "no-unused-vars": [ 1 ]
  }
}
